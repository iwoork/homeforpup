{
  "name": "homeforpup-monorepo",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev --concurrency=20",
    "build": "turbo run build --concurrency=20",
    "start": "turbo run start --concurrency=20",
    "lint": "turbo run lint --concurrency=20",
    "type-check": "turbo run type-check --concurrency=20",
    "clean": "turbo run clean --concurrency=20",
    "dev:adopter": "turbo run dev --filter=adopter-app",
    "dev:breeder": "turbo run dev --filter=breeder-app",
    "build:adopter": "turbo run build --filter=adopter-app",
    "build:breeder": "turbo run build --filter=breeder-app"
  },
  "devDependencies": {
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.12.2",
    "@typescript-eslint/parser": "^8.12.2",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.5.4",
    "turbo": "^1.10.12",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@aws-sdk/client-acm": "^3.901.0",
    "@aws-sdk/client-cloudfront": "^3.901.0",
    "@aws-sdk/client-s3": "^3.901.0"
  }
}
